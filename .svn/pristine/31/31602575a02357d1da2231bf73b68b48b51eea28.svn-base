<ion-header>
  <ion-navbar ion-text color="primary">
    <button ion-button menuToggle>
      <ion-icon name="menu"></ion-icon>
    </button>
    <ion-title>Stores</ion-title>
  </ion-navbar>
</ion-header>
<ion-content padding class="store">
  <ion-list>
    <div *ngFor="let store of stores">
      <ion-list-header>{{store.name}}</ion-list-header>   
      <ion-item>
        <p>{{store.address}}</p>
        <p>{{store.phone}}</p>
        <p>{{store.email}}</p>  
        <!--<button ion-button full (click)="showme(store)">Show Map</button>-->
      </ion-item>      
    </div>   
  </ion-list>    
</ion-content>
